<div class="container latest_container">
  <div class="row">
    <div style="display: flex;">
    <% @latest_reviews.each do |review| %>
    <div style="margin-right: 10px;">
    <%= link_to review_path(review.id) do %>
      <%= image_tag review.get_review_image(190,550), style: "max-width: 200px; max-height: 550px;" %>
    <% end %>
    </div>
  <% end %>
  </div>
  </div>
</div>